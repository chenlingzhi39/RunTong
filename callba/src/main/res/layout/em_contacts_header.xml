<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"

    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical">

    <com.callba.phone.widget.ContactItemView
        android:id="@+id/application_item"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:contactItemImage="@drawable/em_new_friends_icon"
        app:contactItemName="@string/Application_and_notify" />
    <View
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:background="@color/divider_list"/>
    <com.callba.phone.widget.ContactItemView
        android:id="@+id/black_item"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:contactItemImage="@drawable/em_groups_icon"
        app:contactItemName="@string/blacklist" />
    <View
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:background="@color/divider_list"/>
    <com.callba.phone.widget.ContactItemView
        android:id="@+id/nearby_item"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:contactItemImage="@drawable/ease_chat_location_normal"
        app:contactItemName="@string/friend" />
    <View
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:background="@color/divider_list"/>
    <com.callba.phone.widget.ContactItemView
        android:id="@+id/group_item"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:contactItemImage="@drawable/em_groups_icon"
        app:contactItemName="@string/group_chat" />
    <View
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:background="@color/divider_list"/>
   <!-- <com.hyphenate.chatuidemo.widget.ContactItemView
        android:id="@+id/chat_room_item"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:contactItemImage="@drawable/em_groups_icon"
        app:contactItemName="@string/chat_room" />

    <com.hyphenate.chatuidemo.widget.ContactItemView
        android:id="@+id/robot_item"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:contactItemImage="@drawable/em_groups_icon"
        app:contactItemName="@string/robot_chat" />-->

</LinearLayout>
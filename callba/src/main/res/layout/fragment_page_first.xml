<?xml version="1.0" encoding="utf-8"?>
<android.support.percent.PercentRelativeLayout
    android:id="@+id/rootFirstPage"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:ignore="ContentDescription"
    tools:background="@android:color/holo_orange_dark">
    <ImageView
        android:id="@+id/ivFirstImage"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:src="@drawable/s_0_0"
        app:layout_marginTopPercent="25%"
        />
    <ImageView
        android:id="@+id/ivSecondImage"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/s_0_1"
        app:layout_marginTopPercent="18%"
        app:layout_marginLeftPercent="45%"
        />
    <ImageView
        android:id="@+id/ivThirdImage"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/s_0_2"
        app:layout_marginTopPercent="25%"
        app:layout_marginLeftPercent="25%"

        />
    <ImageView
        android:id="@+id/ivFourthImage"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/s_0_3"
        app:layout_marginTopPercent="34%"
        app:layout_marginLeftPercent="40%"
        />
    <ImageView
        android:id="@+id/ivFifthImage"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/s_0_4"
        app:layout_marginTopPercent="30%"
        app:layout_marginLeftPercent="52%"
        />
    <ImageView
        android:id="@+id/ivSixthImage"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/s_0_5"
        app:layout_marginTopPercent="30%"
        app:layout_marginLeftPercent="65%"
        />
    <ImageView
        android:id="@+id/ivSeventhImage"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/s_0_6"
        app:layout_marginTopPercent="40%"
        app:layout_marginLeftPercent="30%"
        />
    <ImageView
        android:id="@+id/ivEighthImage"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/s_0_7"
        app:layout_marginTopPercent="40%"
        app:layout_marginLeftPercent="55%"
        />
    <ImageView
        android:id="@+id/ivNinthImage"

        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:src="@drawable/s_0_8"
        app:layout_marginTopPercent="65%"
        />
</android.support.percent.PercentRelativeLayout>